<h1 class="profile-title">My Profile</h1>

<hr>

<h3 class="profile-subtitle">My Gigs</h3>
<div class="profile-gigs">
<% for (const gig of myGigs) { %>
    <ul>
        <li><a href="/gigs/<%= gig.id %>"><%= gig.name %></a></li>
    </ul>
<% } %>

<hr>

<h3 class="profile-subtitle">Gigs I Am Going To</h3>
<% for (const gig of gigsIAmGoingTo) { %>
    <ul>
        <li><a href="/gigs/<%= gig.id %>"><%= gig.name %></a></li>
    </ul>
<% } %>
</div>

